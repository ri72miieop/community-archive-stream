{
  "name": "community-archive-stream",
  "displayName": "Community Archive Stream",
  "version": "0.0.4",
  "description": "An extension to stream data to the community archive as you use Twitter.",
  "author": "IaimforGOAT",
  "scripts": {
    "dev": "plasmo dev --entry=options --env=.env.development --verbose --tag=dev",
    "build": "plasmo build --env=.env.development --verbose --tag=dev",
    "package": "plasmo package --env=.env.development --tag=dev",
    "tailwind": "npx tailwindcss -i ./input.css -o ./prod.css",
    "tailwind:watch": "npx tailwindcss -i ./input.css -o ./prod.css --watch",
    "dev:firefox": "plasmo dev --target=firefox-mv3",
    "dev:brave": "plasmo dev --target=brave-mv3",
    "build:chrome": "plasmo build --entry=options --env=.env.development",
    "build:firefox": "plasmo build --target=firefox-mv3 --env=.env.development --tag dev",
    "build:brave": "plasmo build --target=brave-mv3",
    "build:prod": "plasmo build --tag=prod --env=.env.production ",
    "build:stage": "plasmo build --tag=stage --env=.env.staging ",
    "buildzip:prod": "plasmo build --tag=prod --env=.env.production --zip",
    "build2:prod": "plasmo build --tag=prod2 --env=.env.production --zip --hoist --no-minify",
    "clean": "if exist .plasmo rd /s /q .plasmo && if exist .parcel-cache rd /s /q .parcel-cache && if exist node_modules rd /s /q node_modules && pnpm install",
    "dev:clean": "pnpm clean && pnpm dev",
    "dev:debug": "plasmo dev --entry=options --env=.env.development --verbose --debug --no-optimize"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@plasmohq/messaging": "0.6.2",
    "@plasmohq/storage": "1.12.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.6",
    "@supabase/supabase-js": "2.39.3",
    "@swc/types": "^0.1.17",
    "async-mutex": "^0.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cmdk": "^1.0.4",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.10",
    "dexie-export-import": "^4.1.4",
    "dexie-react-hooks": "^1.1.7",
    "jszip": "^3.10.1",
    "lucide-react": "^0.453.0",
    "next-themes": "^0.4.6",
    "plasmo": "0.90.3",
    "posthog-js": "^1.184.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^2.0.1",
    "svgo": "^3.0.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "react-chartjs-2": "^5.3.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "https-browserify": "^1.0.0",
    "postcss": "^8.4.47",
    "prettier": "3.2.4",
    "stream-http": "^3.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "permissions": [
      "storage",
      "sidePanel",
      "cookies"
    ],
    "host_permissions": [
      "https://*.twitter.com/*",
      "https://*.x.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html",
          "assets/custom/nopfp2_4832.jpg",
          "prod.css",
          "input.css"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@swc/core",
    "@tailwindcss/oxide",
    "core-js"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
